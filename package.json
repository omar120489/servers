    "yup": "1.7.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.37.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@playwright/test": "^1.56.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.5",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "env-cmd": "11.0.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
